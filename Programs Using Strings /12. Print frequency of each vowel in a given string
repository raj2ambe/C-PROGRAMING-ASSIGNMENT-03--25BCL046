#include <stdio.h>

int main() {
    char str[100] = "Hello RAJ, welcome to programming!";
    int i = 0;
    int a = 0, e = 0, i_count = 0, o = 0, u = 0;

    // Count vowels
    while (str[i] != '\0') {
        switch (str[i]) {
            case 'a': case 'A': a++; break;
            case 'e': case 'E': e++; break;
            case 'i': case 'I': i_count++; break;
            case 'o': case 'O': o++; break;
            case 'u': case 'U': u++; break;
        }
        i++;
    }

    // Print frequencies
    printf("Frequency of vowels:\n");
    printf("A/a: %d\n", a);
    printf("E/e: %d\n", e);
    printf("I/i: %d\n", i_count);
    printf("O/o: %d\n", o);
    printf("U/u: %d\n", u);

    return 0;
}
