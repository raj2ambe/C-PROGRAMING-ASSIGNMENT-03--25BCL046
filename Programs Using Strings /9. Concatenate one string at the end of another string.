#include <stdio.h>

int main() {
    char str1[100] = "Hello ";
    char str2[100] = "RAJ!";
    int i = 0, j = 0;

    // Move to the end of str1
    while (str1[i] != '\0') {
        i++;
    }

    // Append str2 to str1
    while (str2[j] != '\0') {
        str1[i] = str2[j];
        i++;
        j++;
    }
    str1[i] = '\0'; // Null-terminate the result

    printf("Concatenated string: %s\n", str1);

    return 0;
}
