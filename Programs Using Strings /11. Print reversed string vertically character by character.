#include <stdio.h>
#include <string.h>

int main() {
    char str[100] = "Hello RAJ!";
    int i;

    // Find length of string
    int len = strlen(str);

    // Print characters in reverse order, one per line
    for (i = len - 1; i >= 0; i--) {
        printf("%c\n", str[i]);
    }

    return 0;
}
