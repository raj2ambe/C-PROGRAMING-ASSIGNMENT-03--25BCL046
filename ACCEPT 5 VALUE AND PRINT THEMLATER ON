#include <stdio.h>

int main() {
    int values[5];

    // Accepting 5 values
    printf("Enter 5 integer values:\n");
    for(int i = 0; i < 5; i++) {
        printf("Value %d: ", i + 1);
        scanf("%d", &values[i]);
    }

    // Printing the values
    printf("\nYou entered:\n");
    for(int i = 0; i < 5; i++) {
        printf("Value %d: %d\n", i + 1, values[i]);
    }

    return 0;
}
