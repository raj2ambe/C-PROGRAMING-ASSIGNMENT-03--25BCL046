#include <stdio.h>

int main() {
    int arr[100] = {2, 4, 6, 8, 10}; // Initial array
    int n = 5; // Current size of array
    int newNum = 1; // Number to insert at beginning

    // Shift elements to the right
    for (int i = n; i > 0; i--) {
        arr[i] = arr[i - 1];
    }

    // Insert new number at index 0
    arr[0] = newNum;
    n++; // Update size

    // Print updated array
    printf("Updated array:\n");
    for (int i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }

    return 0;
}
