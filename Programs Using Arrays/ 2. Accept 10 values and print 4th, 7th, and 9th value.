#include <stdio.h>

int main() {
    int values[10];

    // Accepting 10 values
    printf("Enter 10 integer values:\n");
    for(int i = 0; i < 10; i++) {
        printf("Value %d: ", i + 1);
        scanf("%d", &values[i]);
    }

    // Printing 4th, 7th, and 9th values
    printf("\nSelected values:\n");
    printf("4th value: %d\n", values[3]);
    printf("7th value: %d\n", values[6]);
    printf("9th value: %d\n", values[8]);

    return 0;
}
