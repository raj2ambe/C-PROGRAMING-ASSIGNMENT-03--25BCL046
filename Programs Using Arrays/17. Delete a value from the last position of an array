#include <stdio.h>

int main() {
    int arr[100] = {10, 20, 30, 40, 50}; // Initial array
    int n = 5; // Current size of array

    // Delete value from last position by reducing size
    n--;

    // Print updated array
    printf("Updated array:\n");
    for (int i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }

    return 0;
}
