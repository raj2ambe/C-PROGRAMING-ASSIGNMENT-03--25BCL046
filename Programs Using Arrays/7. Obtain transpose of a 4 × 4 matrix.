#include <stdio.h>

int main() {
    int matrix[4][4], transpose[4][4];

    // Input original matrix
    printf("Enter elements of 4x4 matrix:\n");
    for(int i = 0; i < 4; i++) {
        for(int j = 0; j < 4; j++) {
            printf("matrix[%d][%d]: ", i, j);
            scanf("%d", &matrix[i][j]);
        }
    }

    // Compute transpose
    for(int i = 0; i < 4; i++) {
        for(int j = 0; j < 4; j++) {
            transpose[j][i] = matrix[i][j];
        }
    }

    // Display transpose
    printf("\nTranspose of the matrix:\n");
    for(int i = 0; i < 4; i++) {
        for(int j = 0; j < 4; j++) {
            printf("%d\t", transpose[i][j]);
        }
        printf("\n");
    }

    return 0;
}
